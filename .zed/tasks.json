[
  //
  //  dotnet build, run
  //
  {
    "label": "Build DotnetAgents/DotnetAgents.sln",
    "command": "dotnet",
    "args": ["build", "DotnetAgents/DotnetAgents.slnx"]
  },
  {
    "label": "build",
    "command": "dotnet",
    "args": [
      "build",
      "/Users/xxx/Repositories/xxx/services/xxx/service/service.csproj"
    ]
  },
  {
    "label": "dotnet build worktree root",
    "command": "dotnet",
    "show_command": true,
    "args": ["build"],
    "cwd": "$ZED_WORKTREE_ROOT"
  },
  {
    "label": "dotnet run worktree root",
    "command": "dotnet",
    "show_command": true,
    "args": ["run"],
    "cwd": "$ZED_WORKTREE_ROOT"
  },
  {
    "label": "dotnet debug worktree root",
    "command": "netcoredbg",
    "show_command": true,
    "args": ["-- dotnet run"],
    "cwd": "$ZED_WORKTREE_ROOT"
  },
  //
  // dev containers
  //
  {
    "label": "DevContainer: Up",
    "command": "devcontainer up - workspace-folder ${ZED_WORKTREE_ROOT}",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "DevContainer: Build",
    "command": "devcontainer build - workspace-folder ${ZED_WORKTREE_ROOT}",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "DevContainer: Exec",
    "command": "devcontainer exec - workspace-folder ${ZED_WORKTREE_ROOT} ${1}",
    "use_new_terminal": true,
    "allow_concurrent_runs": true,
    "reveal": "always"
  },
  {
    "label": "DevContainer: Run User Commands",
    "command": "devcontainer run-user-commands - workspace-folder ${ZED_WORKTREE_ROOT}",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "build: Debug",
    "command": "dotnet",
    "args": [
      "build",
      "DotnetAgents/DotnetAgents.slnx",
      "--configuration",
      "Debug"
    ],
    "cwd": "$ZED_WORKTREE_ROOT",
    "show_command": true,
    "show_summary": true,
    "reveal": "always"
  },
  {
    "label": "build: Release",
    "command": "dotnet",
    "args": [
      "build",
      "DotnetAgents/DotnetAgents.slnx",
      "--configuration",
      "Release"
    ],
    "cwd": "$ZED_WORKTREE_ROOT",
    "show_command": true,
    "show_summary": true,
    "reveal": "always"
  }
]
