{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "dotnet: build",
      "type": "dotnet",
      "task": "build",
      "group": {
        "kind": "build"
      },
      "problemMatcher": ["$msCompile"]
    },
    {
      "label": "build: Debug",
      "type": "shell",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/DotnetAgents/DotnetAgents.slnx",
        "--configuration",
        "Debug"
      ],
      "problemMatcher": ["$msCompile"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "build: Release",
      "type": "shell",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/DotnetAgents/DotnetAgents.slnx",
        "--configuration",
        "Release"
      ],
      "problemMatcher": ["$msCompile"],
      "group": "build"
    }
  ]
}
